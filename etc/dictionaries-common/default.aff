#  In debian the default charset has been changed to latin1

nroffchars      ().\\*
texchars        ()\[]{}<\>\\$*.%

allaffixes on

# normal latin1 encoding
defstringtype "latin1" "tex" ".txt"

wordchars       a       A
stringchar      á       Á
stringchar      à       À
wordchars       [b-e]   [B-E]
stringchar      é       É
stringchar      è       È
wordchars       [f-i]   [F-I]
stringchar      í       Í
stringchar      ì       Ì
wordchars       [j-n]   [J-N]
stringchar      ñ       Ñ
wordchars       o       O
stringchar      ó       Ó
stringchar      ò       Ò
wordchars       [p-u]   [P-U]
stringchar      ù       Ù
stringchar      ú       Ú
stringchar      ü       Ü
wordchars       [v-z]   [V-Z]
stringchar      ç       Ç

# Now the alternative charsets

